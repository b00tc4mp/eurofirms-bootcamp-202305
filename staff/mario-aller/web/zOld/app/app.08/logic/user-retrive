// Devuelve usuario p√∫blico (ret objeto de la lista del usuario email)
var userRetrieve = function (email) {
    var num = users.length
    var user = null

    for (var i = 0; i < num; i++) {
        if (email === users[i].email) {
            user = {}
            user.name = users[i].name
            user.email = users[i].email
        }
    }

    return user
}